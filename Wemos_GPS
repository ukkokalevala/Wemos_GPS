#include <SoftwareSerial.h>

int Rx_pin =D2;  //from Neo-6M to GND or disconected on Nano Board
int Tx_pin =D3;  //from Neo-6M to D2 on Nano Booard
SoftwareSerial SerialGPS(Rx_pin, Tx_pin);

void setup()
{
  Serial.begin(9600);
  SerialGPS.begin(9600);
}

void loop()
{
  while (SerialGPS.available() > 0)
  Serial.write(SerialGPS.read());
  
  
}
